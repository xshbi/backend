{
  "name": "webapp",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "NODE_ENV=production bun dist/index.js",
    "build": "bun build src/index.ts --target bun --outdir ./dist",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/static": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.10",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.22",
    "jsonwebtoken": "^9.0.3",
    "jwt": "^0.2.0",
    "pg": "^8.17.2",
    "postgres": "^3.4.8",
    "prettier": "3.8.1"
  }
}
